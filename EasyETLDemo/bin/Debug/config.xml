  <transforms>
    <transform profilename="ColorsTransformation">Add SNo AutoNumber
Hide hex
hide category
Add ucategory translate(category,'abcdefghijklmnopqrstuvwxyz','ABCDEFGHIJKLMNOPQRSTUVWXYZ')
rename color Color
rename type ColorType</transform>
    <transform profilename="CSV Multiple Transforms">[row=&gt;PatientVisit]
Add SNo AutoNumber
Add DispCode substring-before(Disposition,' ')
filter starts-with(PatientName,'W') or Disposition='05 Eloped'
filter starts-with(PrimaryNurse,'MURRAY') and starts-with(Disposition,'02')
GO
Add SNo1 SNo+1
Add Val 'Test Value'
sort AgeGen
//GO</transform>
    <transform profilename="CSV Onload Transform">Add SetNo 1</transform>
    <transform profilename="FilterCsvFile-H">[row=&gt;PatientDetails]
Add DispCode substring-before(Disposition,' ')
Add Age substring-before(AgeGen,' ')
Add Gender substring-after(AgeGen,'/')
--filter1 starts-with(PatientName,'W') or Disposition='05 Eloped'
--filter1 starts-with(PrimaryNurse,'MURRAY') and starts-with(Disposition,'02')
--filter starts-with(Disposition,'02') and starts-with(PrimaryNurse,'HUDSON')
filter starts-with(MRN,'7')
GO
filter Age&gt;50 and easy:Upper(Gender)='MALE' and starts-with(DispCode,'0')
sort Sno</transform>
    <transform profilename="FilterCsvFile">Add SNo AutoNumber
Add DispCode substring-before(Disposition,' ')
Add Age substring-before(AgeGen,' ')
filter1 starts-with(PatientName,'W') or Disposition='05 Eloped'
filter starts-with(Disposition,'01') and starts-with(AgeGen,'7')</transform>
    <transform profilename="HL7-PID">[PID=&gt;PatientDetails]
Add SNo number</transform>
    <transform profilename="HL7">[MSH=&gt;MessageHeader]
rename msh_0 SegmentName
rename msh_1 Separators
rename MSH_2 SourceOrganization
rename msh_3 SourceApplication
rename MSH_4 TargetOrganization
rename msh_5 TargetApplication
Add SNo AutoNumber
[PID=&gt;PatientData]
rename pid_0 SegmentName
rename PID_1 OccurenceIndex
[PD1=&gt;PatientAdditionalData]
[OBX=&gt;ObservationResults]
rename obx_0 SegmentName
GO
[PatientData]
sort OccurenceIndex data-type='number' order='descending'
[ObservationResults]
sort OBX_1
GO</transform>
    <transform profilename="IIS-DuringLoad-Transforms">[row=&gt;log]
rename Field_0 date
rename Field_1 time
rename Field_2 ClientIP
rename Field_3 Verb
remove Field_4
remove Field_5
rename Field_6 IncomingPort
rename field_7 ClientMachineName
remove Field_8
rename field_9 BrowserInfo
rename field_10 CallURL
rename field_11 ResponseCode
remove field_12
remove Field_13
remove Field_14
Add BrowserName substring-before(Field_9,'/')
Add SNo AutoNumber</transform>
    <transform profilename="IIS-LogFile-Parser">[row=&gt;logrecord]
rename Field_0 Date
rename Field_1 Time
rename Field_2 SourceIP
rename Field_3 CallMethod
rename Field_4 CallURL
rename Field_5 CallParams
rename Field_6 CallPort
GO</transform>
    <transform profilename="IncludePrimaryKey">Add SNo AutoNumber
Add Disp substring(Disposition,1,2)
Add Age substring-before(AgeGen,' ')
HIDE ArrivalMode
Hide AdmitPhysician
Hide Unused
Hide DischargeLoc
Hide Disposition
Hide AgeGen</transform>
    <transform profilename="Insurance_Sample">[row=&gt;Policies]
filter eq_site_limit &gt; 0
orderby policyid</transform>
    <transform profilename="Regex Test">[row=&gt;Data]
filter not(easy:IsEmpty(easy:Replace(FileName,'.',''))) and not(easy:IsEmpty(Size))
sort Size
go
Add SNo AutoNumber
(?=\d)[DateTime] (AM|PM)( )+[Size:(\d,)] [FileName]\r\n</transform>
    <transform profilename="Regex-Followup">[row=&gt;Names]
Add SerialNo autonumber
filter not(translate(FileName,'.','')='') 
sort FileName</transform>
    <transform profilename="Regex-Names">[row=&gt;Names]
Add SerialNo autonumber
Add FullName normalize-space(translate(concat(FirstName,' ',LastName),'abcdefghijklmnopqrstuvwxyz','ABCDEFGHIJKLMNOPQRSTUVWXYZ'))</transform>
  </transforms>
